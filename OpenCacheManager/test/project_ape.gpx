    <groundspeak:cache id="2635" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Mission 1: Devil's Spoon</groundspeak:name>
      <groundspeak:placed_by>Project APE</groundspeak:placed_by>
