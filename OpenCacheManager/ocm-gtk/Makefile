	$(call cp,$(WEBCAM_PNG),$(DESTDIR)$(libdir)/$(PACKAGE)/icons/24x24)
	$(call cp,$(WHERIGO_PNG),$(DESTDIR)$(libdir)/$(PACKAGE)/icons/24x24)
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)/icons/48x48'
	$(call cp,$(CIRCLE_PNG),$(DESTDIR)$(libdir)/$(PACKAGE)/icons/48x48)
