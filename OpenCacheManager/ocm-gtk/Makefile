	$(call cp,$(TRAILHEAD_PNG),$(DESTDIR)$(libdir)/$(PACKAGE)/icons/24x24)
	$(call cp,$(BLUEPIN_SVG),$(DESTDIR)$(libdir)/$(PACKAGE)/icons/scalable)
	$(call cp,$(PARKING_SVG),$(DESTDIR)$(libdir)/$(PACKAGE)/icons/scalable)
	$(call cp,$(TRAILHEAD_SVG),$(DESTDIR)$(libdir)/$(PACKAGE)/icons/scalable)
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)/docs'
	$(call cp,$(LICENCE_TXT),$(DESTDIR)$(libdir)/$(PACKAGE)/docs)
	$(call cp,$(CHANGEHISTORY_TXT),$(DESTDIR)$(libdir)/$(PACKAGE)/docs)
	$(call cp,$(README_TXT),$(DESTDIR)$(libdir)/$(PACKAGE)/docs)
	$(call cp,$(OCMENGINE_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(OCMENGINE_DLL_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(OCM_GTK_EXE_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	mkdir -p '$(DESTDIR)$(bindir)'
	$(call cp,$(OCM_GTK),$(DESTDIR)$(bindir))
	update-mime-database /usr/share/local/mime
	make post-install-local-hook prefix=$(prefix)

