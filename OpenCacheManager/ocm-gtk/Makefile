	$(call cp,$(TRADITIONAL_SVG),$(DESTDIR)$(libdir)/$(PACKAGE)/icons/scalable)
	$(call cp,$(BLUEPIN_PNG),$(DESTDIR)$(libdir)/$(PACKAGE)/icons/24x24)
